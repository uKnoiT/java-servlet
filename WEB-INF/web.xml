<?xml version="1.0" encoding="UTF-8"?>

<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <!-- id="WebApp_ID" version="3.1"> -->

    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server1</servlet-name>
        <servlet-class>p1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server2</servlet-name>
        <servlet-class>p2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server3</servlet-name>
        <servlet-class>p3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server4</servlet-name>
        <servlet-class>p4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server5</servlet-name>
        <servlet-class>p5</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server6</servlet-name>
        <servlet-class>p6_a</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>server6_b</servlet-name>
        <servlet-class>p6_b</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server1</servlet-name>
        <url-pattern>/prac-1/server1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server2</servlet-name>
        <url-pattern>/prac-2/server2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server3</servlet-name>
        <url-pattern>/prac-3/server3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server4</servlet-name>
        <url-pattern>/prac-4/server4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server5</servlet-name>
        <url-pattern>/prac-5/server5</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server6</servlet-name>
        <url-pattern>/prac-6/server6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server6_b</servlet-name>
        <url-pattern>/prac-6/server6_b</url-pattern>
    </servlet-mapping>

</web-app>
